
Variables
x1 in [ 100, 10000] ;
x2 in [ 1000, 10000];
x3 in [ 1000, 10000];
x4 in [10, 1000];
x5 in [ 10, 1000];
x6 in [ 10, 1000] ;
x7 in [ 10, 1000];
x8 in [ 10, 1000];


// minimize obj: x1 + x2 + x3;

constraints

minimize x1+x2+x3;
