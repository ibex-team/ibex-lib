		YY_CURRENT_BUFFER_LVALUE =    \
            ibex_create_buffer(ibexin,YY_BUF_SIZE ); \
	} \
	YY_CURRENT_BUFFER_LVALUE->yy_at_bol = at_bol; \
	}

